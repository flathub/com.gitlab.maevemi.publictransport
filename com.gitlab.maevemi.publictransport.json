{
    "app-id" : "com.gitlab.maevemi.publictransport",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "publictransport",
    "finish-args" : [
        "--device=dri",
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--env=RUST_LOG=publictransport=info,"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "env" : {
            "RUST_BACKTRACE" : "1"
        }
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "publictransport",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.com/maevemi/publictransport-releases/-/raw/main/releases/publictransport-0.2.2.tar.xz",
                    "sha256" : "c56e7d5c924dcf9b5b96ed434e74b2f209ae2c8da83c341c6337b09268c396fc"
                }
            ]
        }
    ]
}
