{
    "app-id" : "com.gitlab.maevemi.publictransport",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "publictransport",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "build-args" : [
            "--share=network"
        ],
        "env" : {
            "RUST_BACKTRACE" : "1",
            "RUST_LOG" : "publictransport=debug"
        }
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "publictransport",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type": "git",
                    "url": "https://gitlab.com/maevemi/publictransport.git",
                    "tag": "v0.1.0",
                    "commit": "e524cf6a6bc8412af7d8a5ddca6431b8b45a4986"
                }
            ]
        }
    ]
}
